<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.runtime.remoting>
		<application>
			<client>
				<wellknown type="Server.clsServer, Server" url="http://localhost/server/server.soap"/>
			</client>
			<channels>
				<channel ref="http">
					<clientProviders>
						<formatter ref="binary"/>
					</clientProviders>
				</channel>
			</channels>
		</application>
	</system.runtime.remoting>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
	</startup>
</configuration>